<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.adm.entity.Bank"
          extends="_local"
          name="bank-view"
          overwrite="true"
          systemProperties="true">
        <property name="contacts"
                  view="_minimal">
            <property name="comment"/>
        </property>
    </view>
    <view class="com.company.adm.entity.Ticket"
          extends="_local"
          name="ticket-view"
          systemProperties="true">
        <property name="contacts"
                  view="_minimal">
            <property name="comment"/>
        </property>
        <property name="interest"
                  view="_minimal"/>
        <property name="responsibleManager"
                  view="_minimal"/>
        <property name="history"
                  view="_local">
            <property name="user"
                      view="_minimal"/>
        </property>
        <property name="sourceDescription"
                  view="_minimal"/>
        <property name="questionnaire"
                  view="_local">
            <property name="bankTickets"
                      view="_local">
                <property name="bank"
                          view="_minimal">
                    <property name="address"/>
                    <property name="contacts"
                              view="_minimal">
                        <property name="comment"/>
                    </property>
                </property>
            </property>
            <property name="currentLoans"
                      view="_local"/>
            <property name="incomes"
                      view="_local"/>
        </property>
        <property name="status"
                  view="_local">
        </property>
        <property name="customScheduler"
                  view="_minimal">
            <property name="workedOut"/>
            <property name="comment"/>
        </property>
    </view>
    <view class="com.company.adm.entity.contracts.Contract"
          extends="_local"
          name="contract-view"
          systemProperties="true">
        <property name="contractor"
                  view="contractor-view">
        </property>
        <property name="services"
                  view="_minimal">
            <property name="cost"/>
            <property name="state"
                      view="_minimal"/>
            <property name="service">
                <property name="name"/>
            </property>
        </property>
        <property name="responsibleManager"
                  view="_minimal"/>
        <property name="history"
                  view="_local">
            <property name="user"
                      view="_minimal"/>
        </property>
        <property name="analytics"
                  view="_local">
            <property name="ticketToBanks"
                      view="bankTicketLine-view">
            </property>
            <property name="suitableBanks"
                      view="suitableBank-view">
                <property name="bank"
                          view="_minimal">
                    <property name="contacts"
                              view="_minimal"/>
                </property>
                <property name="documents">
                    <property name="name"/>
                </property>
                <property name="analytics">
                    <property name="number"/>
                    <property name="dateCreation"/>
                    <property name="contract">
                        <property name="contractNumber"/>
                    </property>
                </property>
                <property name="state">
                    <property name="name"/>
                    <property name="position"/>
                </property>
            </property>
            <property name="contract"
                      view="_minimal"/>
            <property name="currentLoans"
                      view="_local"/>
        </property>
        <property name="ticket"
                  view="ticket-view"/>
        <property fetch="AUTO"
                  name="questionnaire"
                  view="questionnaire-view">
        </property>
        <property name="internalFiles"
                  view="_local"/>
        <property name="source"
                  view="_minimal"/>
        <property name="schedules"
                  view="_minimal">
            <property name="workedOut"/>
            <property name="comment"/>
        </property>
    </view>
    <view class="com.company.adm.entity.contracts.Contractor"
          extends="_local"
          name="contractor-view">
        <property name="contacts"
                  view="_minimal">
            <property name="bank"
                      view="_minimal">
                <property name="contacts"
                          view="_minimal"/>
            </property>
            <property name="comment"/>
        </property>
        <property name="files"
                  view="_local"/>
    </view>
    <view class="com.company.adm.entity.contracts.ContractServiceLine"
          extends="_local"
          name="contractServiceLine-view"
          systemProperties="true">
        <property name="service"
                  view="service-view">
        </property>
        <property name="state"
                  view="_minimal"/>
    </view>
    <view class="com.company.adm.entity.contracts.Service"
          extends="_local"
          name="service-view"
          systemProperties="true">
    </view>
    <view class="com.company.adm.entity.contracts.Comment"
          extends="_local"
          name="comment-view"
          systemProperties="true">
        <property name="user"
                  view="_local"/>
        <property name="contract"
                  view="contract-view"/>
        <property name="ticket"
                  view="ticket-view"/>
    </view>
    <view class="com.company.adm.entity.contracts.analytics.Analytics"
          extends="_local"
          name="analytics-view"
          systemProperties="true">
        <property name="suitableBanks"
                  view="suitableBank-view">
        </property>
        <property name="ticketToBanks"
                  view="bankTicketLine-view">
        </property>
        <property name="contract"
                  view="_minimal">
            <property name="createDate"/>
        </property>
        <property name="currentLoans"
                  view="_local"/>
    </view>
    <view class="com.company.adm.entity.contracts.analytics.SuitableBank"
          extends="_local"
          name="suitableBank-view"
          systemProperties="true">
        <property name="documents"
                  view="_minimal"/>
        <property name="bank"
                  view="_minimal">
            <property name="address"/>
        </property>
        <property name="analytics"
                  view="_minimal">
            <property name="contract"
                      view="_minimal">
                <property name="contractor">
                    <property name="name"/>
                </property>
            </property>
        </property>
        <property name="state"
                  view="_local"/>
    </view>
    <view class="com.company.adm.entity.contracts.ServiceStatus"
          extends="_local"
          name="serviceStatus-view"
          systemProperties="true">
        <property name="directionsList"/>
    </view>
    <view class="com.company.adm.entity.scheduler.CustomScheduler"
          extends="_local"
          name="customScheduler-view">
        <property name="ticket"
                  view="ticket-view"/>
        <property name="contract"
                  view="contract-view">
            <property name="history"
                      view="_local">
                <property name="user"
                          view="_local"/>
            </property>
            <property name="questionnaire"
                      view="questionnaire-view">
                <property name="bankTickets"
                          view="_minimal">
                    <property name="bank">
                        <property name="name"/>
                        <property name="address"/>
                    </property>
                </property>
            </property>
            <property name="analytics"
                      view="_local">
                <property name="ticketToBanks"
                          view="bankTicketLine-view">
                    <property name="bank"
                              view="_local">
                        <property name="contacts"
                                  view="_minimal">
                            <property name="comment"/>
                        </property>
                    </property>
                </property>
            </property>
        </property>
    </view>
    <view class="com.company.adm.entity.contracts.analytics.BankTicketLine"
          extends="_local"
          name="bankTicketLine-view">
        <property name="bank"
                  view="_local">
            <property name="contacts"
                      view="_minimal">
                <property name="comment"/>
            </property>
        </property>
    </view>
    <view class="com.company.adm.entity.Questionnaire"
          extends="_local"
          name="questionnaire-view"
          systemProperties="true">
        <property name="bankTickets"
                  view="_local">
            <property name="bank"
                      view="_minimal">
                <property name="address"/>
                <property name="contacts"
                          view="_minimal">
                    <property name="comment"/>
                </property>
            </property>
        </property>
        <property name="incomes"
                  view="_local"/>
        <property name="currentLoans"
                  view="_local"/>
    </view>
    <view class="com.company.adm.entity.Ticket"
          extends="_minimal"
          name="ticketOpt-view">
        <property name="interest"
                  view="_minimal"/>
        <property name="source"/>
        <property name="status"
                  view="_minimal"/>
        <property name="statusDateTime"/>
        <property name="responsibleManager"
                  view="_minimal"/>
    </view>
    <view class="com.company.adm.entity.scheduler.CustomScheduler"
          extends="_local"
          name="customSchedulerTicketOpt-view">
        <property name="ticket"
                  view="_minimal">
            <property name="responsibleManager"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.company.adm.entity.scheduler.CustomScheduler"
          extends="_local"
          name="customSchedulerContractOpt-view">
        <property name="contract"
                  view="_minimal">
            <property name="responsibleManager"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.company.adm.entity.contracts.Contract"
          extends="_minimal"
          name="contractOpt-view">
        <property name="responsibleManager"
                  view="_minimal"/>
        <property name="contractEvent"/>
        <property name="eventTime"/>
    </view>
    <view class="com.company.adm.entity.Ticket"
          extends="_minimal"
          name="ticketOptBrowser-view"
          systemProperties="true">
        <property name="interest"
                  view="_minimal"/>
        <property name="sourceDescription"
                  view="_minimal"/>
        <property name="status"
                  view="_minimal"/>
        <property name="statusDateTime"/>
        <property name="lastCall"/>
        <property name="lastMeeting"/>
        <property name="responsibleManager"
                  view="_minimal"/>
        <property name="characteristic"/>
        <property name="face"/>
        <property name="source"/>
    </view>
    <view class="com.company.adm.entity.contracts.Contract"
          extends="_minimal"
          name="contractOptBrowser-view"
          systemProperties="true">
        <property name="contractStatus"/>
        <property name="direction"/>
        <property name="amount"/>
        <property name="createDate"/>
        <property name="responsibleManager"
                  view="_minimal"/>
        <property name="source"
                  view="_minimal"/>
        <property name="contractEvent"/>
        <property name="eventTime"/>
        <property name="ticket"
                  view="_minimal">
            <property name="source"/>
        </property>
    </view>
    <view class="com.company.adm.entity.contracts.Contract"
          extends="_minimal"
          name="contractOptForTicketEdit-view">
        <property name="contractStatus"/>
        <property name="direction"/>
        <property name="compensation"/>
        <property name="amount"/>
        <property name="createDate"/>
    </view>
</views>
